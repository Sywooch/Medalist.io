/* container */
.container
    padding-top 20px 
    padding-bottom 20px
    background-color rgba(CMain, 0.02)
    &-cols
        white-space nowrap
    &-col 
        @extends .dib_t
        white-space normal
        &-1
            width 270px
            margin-right 50px
        &-2
            width 790px
    &-menu 
        &-list 
            margin 0
            padding 0
            list-style none
            border-radius 8px 
            overflow hidden
            &-li
                background-color white
                border-bottom 1px solid rgba(CBlack, 0.1)
                position relative
                &-active 
                    background-color CSub
            &-link 
                display block
                padding 25px 
                font-size 18px 
                color CBlack
                text-decoration none
                -webkit-transition all 250ms linear 
                -o-transition all 250ms linear 
                transition all 250ms linear 
                &:hover 
                    background-color rgba(CBlack, 0.03)
            &-meta 
                position absolute
                right 0
                top 0 
                height 100%
                &-counter 
                    color CMain
                    padding 25px 13px
                &-add 
                    display block 
                    width 43px
                    height 43px
                    box-sizing border-box
                    overflow hidden
                    position relative
                    border 5px rgba(CMain, 0.2) solid
                    border-radius 22px
                    margin 13px 11px
                    &:hover
                        -webkit-transform scale(1.1)
                        -ms-transform scale(1.1)
                        -o-transform scale(1.1)
                        transform scale(1.1)
                    &-plus 
                        position absolute
                        background-color CMain
                        color white 
                        font-size 28px
                        width 100%
                        height 100%
                        text-align center
                    
                
/* . container end */


