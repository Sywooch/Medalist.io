BoxShadow = 0 0 5px rgba(CBlack, 0.1);
.js-hidden
    display none
border-radius(n)
    -webkit-border-radius n
    -moz-border-radius n
    border-radius n
    
text-shadow(n)
    -webkit-text-shadow n
    -moz-text-shadow n
    -ms-text-shadow n
    -o-text-shadow n
