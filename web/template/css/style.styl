.tabs
  font-size: 18px

.tabs-li
  vertical-align: top

.tabs-li:first-child
  margin-left: 8px

.tabs-li > div
  top: 33px
  bottom: 0
  left: 0
  width: 100%
  padding: 8px
  overflow: auto
  background: #fff
  -moz-box-sizing: border-box
  -webkit-box-sizing: border-box
  box-sizing: border-box

.tabs input[type="radio"] + label
  margin: 0 0px 0 0
  padding: 0 18px
  line-height: 32px
  border-bottom: solid 2px #cecfd3
  text-align: center
  border-radius: 5px 5px 0 0
  cursor: pointer
  -moz-user-select: none
  -webkit-user-select: none
  user-select: none

.tabs input[type="radio"]:checked + label
  z-index: 1
  background: #fff
  border-bottom: solid 2px #8a44ff
  cursor: default
  color: #8a44ff

.tabs input[type="radio"]
  opacity: 0

.tabs input[type="radio"] ~ div
  display: none

.tabs input[type="radio"]:checked:not(:disabled) ~ div
  display: block

.tabs input[type="radio"]:disabled + label
  opacity: .5
  cursor: no-drop

