    &-list 
        color inherit 
        position relative
        &:after
            content " "
            width 3px
            height 100%
            left 47px
            top 0
            position absolute
            z-index -1
            background-color rgba(CBlack, 0.05)
        
    &-block 
        @extends .simplebox
        position relative
        margin-bottom 50px
        
        &-category 
            @extends .dib_base
            margin-right 10px
        &-tags 
            @extends .dib_base
            &-tag
                font-size 12px
                color CGrayText
        &-meta 
            position absolute
            left 0
            top -15px
            left 20px
        &-content 
            @extends .simplebox-padding 
            &-title 
                font-size 24px 
                color CMain
                font-weight bold
                margin-bottom 17px
                line-height 1.3em
                /*width 640px*/
            &-nav-serv
                @extends .dib_t
                /*float right*/
            &-info
                color CGrayText
                font-size 12px
                white-space nowrap
                margin-bottom 20px
                &-date 
                    @extends .dib_t
                    margin-right 10px
                &-status 
                    @extends .dib_t
            &-description
                line-height 1.65em
                margin-bottom 30px
                margin-top 30px
            &-bar 
                width 212px
            &-cols 
                white-space nowrap
            &-col 
                @extends .dib_t
                white-space normal
                &-1 
                    width 450px
                    margin-right 20px
                &-2
                    width 240px
            &-header
                &-cols 
                    white-space nowrap
                &-col 
                    @extends .dib_t
                    white-space normal
                    &-1 
                       width 90%
                       margin-right 20px
                    &-2
                       width 50%
        
        /* achievement-block-user */
        &-user 
            white-space nowrap
            color black 
            text-decoration none
            &-link 
                @extends .dib_m
                color black
                text-decoration none 
                width 350px 
            &-pic 
                @extends .dib_m
                width 75px 
                height 75px 
                border-radius 40px 
                overflow hidden
                background-repeat no-repeat
                background-position center
                background-size cover
            &-name 
                @extends .dib_m
                font-size 18px
                font-weight bold 
                margin-left 20px
            &-allachievements
                @extends .dib_m
            
            
                    
        &-comments 
            text-align right
            font-size 14px
                    
        &-info 
            background-color rgba(CBlack, 0.02)
            padding 15px 30px 
            font-size 14px
        &-rewards
            white-space nowrap
            margin-bottom 20px
            &-reward 
                @extends .dib_m
        &-images 
            white-space nowrap
            margin-bottom 30px
            &-image 
                @extends .dib_t
                

        /* SMALL INVARIATION */
        &--small 
            margin-left 90px
            opacity 0.7
            & .achievement-block-content-title
                font-size 18px 
                margin-bottom 5px
            & .achievement-block-content-description
                font-size 14px
                margin-bottom 30px
                margin-top 30px
            & .achievement-block-content-info
                margin-bottom 12px
            & .achievement-block-content-col-1
                width 454px
            & .achievement-block-content-col-2
                width 150px
            & .achievement-block-images
                text-align right
                margin-bottom 8px
                &-picture-small 
                   background-size cover
                   float right
                   height 150px
                   width 150px
                   margin-top 30px
        
    &-share
        padding 25px
        
    &-attachedquest
        &-block 
            @extends .simplebox-padding
            position relative
            background-size cover 
            background-position center
            color white
            min-height 20px
            
            &-curtain
                position absolute
                left 0
                top 0
                width 100%
                height 100%
                background-color rgba(black, 0.5)
                z-index 10
        &-info 
            @extends .dib_t
            position relative
            z-index 20
            width 340px
            margin-right 20px
            &-link 
                color CSub
        &-rewards
            @extends .dib_t
            position relative
            z-index 21
            width 200px
            &-list 
                &-link 
                    color CSub
       
