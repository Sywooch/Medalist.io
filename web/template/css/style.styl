.dib
    display inline-block
    *display inline 
    zoom 1
.dib_t
    @extends .dib
    vertical-align top
.dib_m
    @extends .dib
    vertical-align middle
.dib_b
    @extends .dib
    vertical-align bottom
.dib_base
    @extends .dib
    vertical-align baseline
.dib_icon 
    @extends .dib_base
    background-repeat no-repeat
    background-position center
    width 16px
    height 16px
.inp-border
    border rgba(CBlack, 0.2) 1px solid   
    box-shadow 0 0 7px rgba(CBlack, 0.1)
    box-sizing border-box
    padding 8px
.simplebox    
    box-shadow BoxShadow
    box-sizing border-box
    border-radius 4px
    background-color white
    line-height 1em
    &-padding 
