FRoboto = "Roboto", sans-serif
C_Red = #ff2d0b
C_Blue = #04a9f4
C_Purple = #8a44ff
C_Yell = #ffe212

/*CMain = CBlue*/
CMain = C_Purple
CAccent = C_Red
CSub = C_Yell
CBlack = #31333e
CGray = #a7a7a7
CGrayText = #9a9ba0

BoxShadow = 0 0 5px rgba(CBlack, 0.1);

border-radius(n)
    -webkit-border-radius n
    -moz-border-radius n
    border-radius n
    
text-shadow(n)
    -webkit-text-shadow n
    -moz-text-shadow n
    -ms-text-shadow n
    -o-text-shadow n
    text-shadow n

.dib
    display inline-block
    *display inline 
    zoom 1
.dib_t
    @extends .dib
    vertical-align top
.dib_m
    @extends .dib
    vertical-align middle
.dib_b
    @extends .dib
    vertical-align bottom
.dib_base
    @extends .dib
    vertical-align baseline
.dib_icon 
    @extends .dib_base
    background-repeat no-repeat
    background-position center
    width 16px
    height 16px
.inp-border
    border rgba(CBlack, 0.2) 1px solid   
    box-shadow 0 0 7px rgba(CBlack, 0.1)
    box-sizing border-box
    padding 8px
.simplebox    
    box-shadow BoxShadow
    box-sizing border-box
    border-radius 4px
    background-color white
    line-height 1em
    &-padding 
        padding 39px 36px 
